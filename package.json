{
  "name": "stargate-js-mono",
  "version": "1.0.0",
  "description": "Monorepo for js code",
  "workspaces": [
    "./libs/GateCore",
    "./libs/GateDevice",
    "./libs/GateRouter",
    "./libs/GateViewModel",
    "./apps/NetworkScanner",
    "./examples/TestDevice"
  ],
  "scripts": {
    "build:scanner": "npm -w apps/NetworkScanner run build",
    "build:core": "npm -w libs/GateCore run build",
    "build:device": "npm -w libs/GateDevice run build",
    "build:router": "npm -w libs/GateRouter run build",
    "build:model": "npm -w libs/GateViewModel run build",
    "build:libs": "run-s build:core build:device build:router build:model",
    "build": "run-s build:libs build:scanner",
    "dev:scanner": "npm -w apps/NetworkScanner run dev",
    "device:test": "npm -w examples/TestDevice run start"
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5"
  },
  "author": "Micha≈Ç Marchel",
  "license": "ISC"
}
